{
  "manifest_version": 3,
  "name": "UDAN-React-Library",
  "version": "0.9.0",
  "content_scripts": [
    {
      "js": [
        "headers.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://*/*"
      ]
    },
    {
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end",
      "js": [
        "index.js"
      ],
      "css": [

      ],
      "media": []
    }
  ],
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": [
    "identity",
    "identity.email",
    "storage",
    "tabs",
    "activeTab"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "index.js",
        "assets/*",
        "./*"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}
