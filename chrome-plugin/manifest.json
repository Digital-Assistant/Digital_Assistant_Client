{
  "manifest_version": 3,
  "name": "Universal Digital Assistant (Beta)",
  "version": "0.1.8.2880",
  "description": "Building an Extension for custom user navigation which helps in assisting users to navigate to right place.",
  "permissions": [
    "background",
    "storage",
    "identity",
    "identity.email",
    "webRequest",
    "declarativeNetRequest",
    "tabs",
    "activeTab"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "content_scripts": [
    {
      "matches": [
        "*://localhost/*",
        "*://*.nistapp.com/*",
        "*://*.rknowsys.com/*",
        "*://*.atlassian.net/*",
        "*://*.vantagecircle.com/*",
        "*://*.vantagecircle.co.in/*",
        "*://*.vantagecircle.in/*",
        "*://*.sportsplus-qaf.app/*",
        "*://*.zigtal.com/*",
        "*://*.discoversaudi.sa/*",
        "*://*.nanohealthplan.com/*",
        "*://*.nanohealth.in/*"
      ],
      "js": [
        "js/injectscriptmodifier.js"
      ],
      "all_frames": false,
      "run_at": "document_start"
    },
    {
      "matches": [
        "*://localhost/*",
        "*://*.nistapp.com/*",
        "*://*.rknowsys.com/*",
        "*://*.atlassian.net/*",
        "*://*.vantagecircle.com/*",
        "*://*.vantagecircle.co.in/*",
        "*://*.vantagecircle.in/*",
        "*://*.sportsplus-qaf.app/*",
        "*://*.zigtal.com/*",
        "*://*.discoversaudi.sa/*",
        "*://*.nanohealthplan.com/*",
        "*://*.nanohealth.in/*"
      ],
      "js": [
        "js/injectsdk.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "js/*",
        "assets/*",
        "css/*",
        "images/*",
        "themes/*"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "background": {
    "service_worker":"js/background.js"
  },
  "oauth2": {
    "client_id": "972566460301-70191ibf29gvdbutqoa41a4iva9ovejl.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/contacts.readonly"
    ]
  },
  "icons": {
    "16": "images/nist-logo-16x16.png",
    "32": "images/nist-logo-32x32.png",
    "48": "images/nist-logo-48x48.png",
    "128": "images/nist-logo-128x128.png"
  }
}
