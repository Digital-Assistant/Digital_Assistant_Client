{
  "name": "UDAN Extension",
  "description": "UDAN Initial Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "content_security_policy": "script-src \"self\" \"unsafe-eval\"; object-src \"self\"",
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://blank.org/"
      ],
      "run_at": "document_start",
      "js": [],
      "css": [],
      "media": []
    },
    {
      "matches": [
        "https://blank.org/"
      ],
      "run_at": "document_end",
      "js": [],
      "css": [],
      "media": []
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}