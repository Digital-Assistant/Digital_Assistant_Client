{
  "manifest_version": 3,
  "name": "UDAN-React-Library",
  "version": "0.1.1",
  "content_scripts": [
    {
      "matches": [
        "*://localhost/*",
        "*://*.nistapp.com/*",
        "*://*.rknowsys.com/*",
        "*://*.atlassian.net/*",
        "*://*.vantagecircle.com/*",
        "*://*.vantagecircle.co.in/*",
        "*://*.vantagecircle.in/*",
        "*://*.sportsplus-qaf.app/*",
        "*://*.zigtal.com/*",
        "*://*.discoversaudi.sa/*",
        "*://*.nanohealthplan.com/*",
        "*://*.nanohealth.in/*"
      ],
      "run_at": "document_end",
      "js": [
        "index.js"
      ],
      "css": [
        "assets/App.min.css"
      ],
      "media": []
    }
  ],
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": [
    "identity",
    "identity.email",
    "storage",
    "tabs",
    "activeTab"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/bundle.js",
        "index.js",
        "assets/App.min.css",
        "assets/logo.svg"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
