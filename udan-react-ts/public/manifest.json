{
  "manifest_version": 2,
  "version": "0.0.1",
  "short_name": "Udan React TS Ext",
  "name": "Udan React TS Extension",
  "description": "React TS Extension",
  "browser_action": {},
  "web_accessible_resources": ["popup.html","logo.html","util.js"],
  "background": {
     "scripts": ["background.bundle.js", "util.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.bundle.js"],
      "all_frames": false,
	    "run_at":"document_start"
    }
  ],
  "icons": {
    "16": "img/nist-logo-16x16.png",
    "48": "img/nist-logo-48x48.png",
    "128": "img/nist-logo-128x128.png"
  },
  "options_page": "options.html",
  "permissions": ["background", "util","storage", "identity","identity.email","webRequest","webRequestBlocking","http://*/*", "https://*/*"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
